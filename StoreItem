package system;
import java.util.Scanner;

public class StoreItem {
	private String code;
	private String name;
	private int quantity;
	
	public StoreItem(String code, String name, int quantity) {
		this.code = code;
		this.name = name;
		this.quantity = quantity;
		
		}
	@Override
	public String toString() {
		return code + " | " + name + " | " + quantity;
		
		
	}
}
